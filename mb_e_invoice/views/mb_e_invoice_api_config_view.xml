<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="view_mb_e_invoice_api_config_form" model="ir.ui.view">
            <field name="name">view.mb.e.invoice.api.config.form</field>
            <field name="model">mb.e.invoice.api.config</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="Reason Invoice">
                    <sheet>
                        <group>
                            <field name="name"/>
                            <field name="url"/>
                            <field name="active"/>
                            <field name="description"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="view_mb_e_invoice_api_config_tree" model="ir.ui.view">
            <field name="name">view.mb.e.invoice.api.config.tree</field>
            <field name="model">mb.e.invoice.api.config</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                    <field name="url"/>
                    <field name="active"/>
                </tree>
            </field>
        </record>

        <record id="action_mb_e_invoice_api_config_invoice" model="ir.actions.act_window">
            <field name="name">Api Configs</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">mb.e.invoice.api.config</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
        </record>
    </data>
</odoo>

