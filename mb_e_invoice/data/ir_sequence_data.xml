<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="seq_cf_contract" model="ir.sequence">
            <field name="name">CloudFone Contract</field>
            <field name="code">cf.contract</field>
            <field name="suffix">/%(range_year)s/HĐCF</field>
            <field name="number_next" eval="1"/>
            <field name="number_increment" eval="1"/>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
            <field name="use_date_range" eval="True"/>
        </record>

        <record id="seq_cf_contract_appendix" model="ir.sequence">
            <field name="name">CloudFone Contract Appendix</field>
            <field name="code">cf.contract.appendix</field>
            <field name="suffix">/%(range_year)s/PLHĐCF</field>
            <field name="number_next" eval="1"/>
            <field name="number_increment" eval="1"/>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
            <field name="use_date_range" eval="True"/>
        </record>
    </data>
</odoo>
