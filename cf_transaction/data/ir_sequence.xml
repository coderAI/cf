<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="seq_cf_bank_transaction" model="ir.sequence">
            <field name="name">Bank Transaction</field>
            <field name="code">bank.transaction</field>
            <field name="prefix">MANUAL-</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_cf_payment" model="ir.sequence">
            <field name="name">CloudFone Payment</field>
            <field name="code">account.payment.sale</field>
            <field name="prefix">%(range_year)s/</field>
            <field name="suffix">-PT</field>
            <field name="number_next" eval="1"/>
            <field name="number_increment" eval="1"/>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
            <field name="use_date_range" eval="True"/>
        </record>

        <record id="seq_cf_payment_purchase" model="ir.sequence">
            <field name="name">CloudFone Payment Purchase</field>
            <field name="code">account.payment.purchase</field>
            <field name="prefix">%(range_year)s/</field>
            <field name="suffix">-PC</field>
            <field name="number_next" eval="1"/>
            <field name="number_increment" eval="1"/>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
            <field name="use_date_range" eval="True"/>
        </record>

    </data>
</odoo>
